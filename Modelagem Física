drop database if exists Situação;
create database Situação;

drop table if EXISTS situação;

-- Criação da tabela Situação
CREATE TABLE Situação (
    ID_Situação INT PRIMARY KEY IDENTITY,
    Data_de_Divulgação DATE,
    Resultado VARCHAR(50),
    ID_Aluno INT,
    FOREIGN KEY (ID_Aluno) REFERENCES Aluno(ID_Aluno)
);

select * from Situação

