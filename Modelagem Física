drop database if exists Turma;
create database Turma;

drop table if EXISTS turma;

-- Criação da tabela Turma
CREATE TABLE Turma (
    ID_Turma INT PRIMARY KEY IDENTITY,
    Ano_Letivo INT,
    Horario VARCHAR(50),
    Sala_de_Aula VARCHAR(50),
    Numero_de_Alunos_Suportados INT
);

